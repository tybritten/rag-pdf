{
  "autoscaling": false,
  "input": {
    "pfs": {
      "glob": "/",
      "name": "data",
      "repo": "parse-docs"
    }
  },
  "pipeline": {
    "name": "embed-docs"
  },
  "transform": {
    "cmd": [
      "/bin/bash",
      "-C"
    ],
    "env": {
      "PYTHON_UNBUFFERED": "1"
    },
    "image": "vmtyler/pdk:embed-v0.2.3",
    "stdin": [
      "python3 embed.py --data-path /pfs/data --emb-model-path http://embed.mlis.svc.cluster.local/v1 --path-to-db /pfs/out/ --translate-model '/mnt/models/Meta-Llama-3.1-8B-Instruct/' --model-url http://llama3.mlis.rag.psdc.lan/v1"
    ]
  }
